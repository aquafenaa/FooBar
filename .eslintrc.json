{  
  "env" : {
    "browser": true,
    "es2020": true,
    "node": true
  },
  "extends": [
    "airbnb",
    "airbnb-typescript"
  ],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "consistent-return": 0,
    "no-param-reassign": 0,
    "max-len": 0,
    "no-console": 0,
    "object-curly-newline": 0
  }
}
